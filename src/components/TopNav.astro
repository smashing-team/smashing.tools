---
import CategorySwitcher from '@/components/CategorySwitcher.astro';
import CommandMenu from '@/components/react/CommandMenu';
import type { CategorySlugs } from '@/consts';

interface Props {
  activeCategory: CategorySlugs;
}

const { activeCategory } = Astro.props;
---

<div class="pb-2 lg:px-4">
  <CommandMenu client:idle />
  <CategorySwitcher activeCategory={activeCategory} />
</div>
